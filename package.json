{
  "name": "markdown-log",
  "version": "1.0.0",
  "description": "Markdown Log Tracker",
  "main": "commands/mlog.js",
  "scripts": {
    "lint": "eslint commands utils db --ext .js",
    "test": "nyc mocha --compilers js:babel-core/register --reporter spec test/**/*.test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:mocha": "mocha --compilers js:babel-core/register --reporter spec test/**/*.test.js",
    "test:watch": "mocha --compilers js:babel-core/register --reporter spec test/**/*.test.js --watch",
    "mocha": "mocha --compilers js:babel-core/register",
    "eslint": "eslint",
    "nyc": "nyc",
    "preversion": "npm run lint && npm test",
    "version": "git add commands db utils",
    "postversion": "git push && git push --tags",
    "mlog": "DEBUG=*,-babel node -r babel-register commands/mlog.js"
  },
  "author": {
    "name": "Jonathan Keller",
    "email": "keller.jonathan@gmail.com"
  },
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/kellerj/mlog.git"
  },
  "bin": {
    "mlog": "./commands/mlog.js"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "commander": "^2.11.0",
    "coveralls": "^3.0.0",
    "date-fns": "^1.29.0",
    "debug": "^3.1.0",
    "minimist": "^1.2.0",
    "sprintf-js": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.2"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 90,
    "statements": 90,
    "functions": 100,
    "branches": 75,
    "reporter": [
      "lcov",
      "text-summary",
      "text",
      "html"
    ],
    "watermarks": {
      "lines": [
        75,
        90
      ],
      "functions": [
        90,
        100
      ],
      "branches": [
        50,
        75
      ],
      "statements": [
        75,
        90
      ]
    },
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
